{
  "version": 2,
  "builds": [
    { "src": "api/send.js", "use": "@vercel/node" },
    { "src": "api/webhook.js", "use": "@vercel/node" },
    { "src": "api/stream.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
